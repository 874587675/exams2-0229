<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.mapper.PostsMapper">
    
    <insert id="insertPosts">
        INSERT INTO posts(post_id,user_id,content,picture,location,created_time)
        values (#{postId},#{userId},#{content},#{picture},#{location},#{createdTime})
    </insert>
    
    <select id="getPosts" resultType="org.example.domain.Posts">
        
    </select>

</mapper>
